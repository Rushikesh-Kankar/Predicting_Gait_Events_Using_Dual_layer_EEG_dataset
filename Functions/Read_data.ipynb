   "source": [
    "import mne\n",
    "\n",
    "def load_data(event_id, file_path):\n",
    "    all_epochs = []\n",
    "    for event in event_id:\n",
    "        epochs = mne.read_epochs(f'{file_path}/my_epochs_{event}-epo.fif')\n",
    "        all_epochs.append(epochs)\n",
    "    all_data = mne.concatenate_epochs(all_epochs)\n",
    "    return all_data"
   ]
  }
